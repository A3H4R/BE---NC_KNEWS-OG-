{
  "name": "BE2-NC-Knews",
  "version": "1.0.0",
  "description": "## Northcoders News API",
  "main": "index.js",
  "scripts": {
    "start": "node listen.js",
    "seed:prod": "NODE_ENV=production DB_URL=$(heroku config:get DATABASE_URL) knex seed:run",
    "migrate:latest:prod": "NODE_ENV=production DB_URL=$(heroku config:get DATABASE_URL) knex migrate:latest",
    "migrate:rollback:prod": "NODE_ENV=production DB_URL=$(heroku config:get DATABASE_URL) knex migrate:rollback",
    "test": "mocha spec && npm run lint:fix",
    "lint": "eslint ./",
    "pre-commit": "npm t",
    "posttest": "npm run lint",
    "make-migration": "knex migrate:make",
    "migrate-latest": "knex migrate:latest",
    "migrate-rollback": "knex migrate:rollback",
    "seed-db": "npm run migrate-rollback && npm run migrate-latest && knex seed:run",
    "setup-dbs": "psql -f db/setup.sql",
    "dev": "nodemon listen.js",
    "lint:fix": "eslint ./ --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/northcoders/BE2-NC-Knews.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/northcoders/BE2-NC-Knews/issues"
  },
  "homepage": "https://github.com/northcoders/BE2-NC-Knews#readme",
  "dependencies": {
    "allow-methods": "^2.0.0",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "knex": "^0.19.5",
    "mocha": "^5.2.0",
    "pg": "^7.8.0",
    "pg-promise": "^8.5.5",
    "supertest": "^3.4.2"
  },
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^1.1.4",
    "nodemon": "^1.18.9"
  }
}
